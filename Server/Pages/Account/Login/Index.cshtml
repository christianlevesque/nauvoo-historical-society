@page
@using Server.Pages.Account.Login
@model Server.Pages.Account.Login.Index

@section Seo
{
	<component type="typeof(HeadOutlet)" render-mode="Static"/>
}

@section Scripts
{
	<script type="application/javascript"
	        src="/js/login.js"
	        defer>
	</script>
	<script type="application/javascript"
	        src="/js/mud-shim.js"
	        defer></script>
}

<component type="typeof(Login)"
           render-mode="Static"
           param-ErrorMessage="@Model.ErrorMessage"
           param-Model="@Model.Login"/>